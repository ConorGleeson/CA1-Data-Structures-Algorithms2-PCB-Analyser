<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="536.0" prefWidth="600.0" stylesheets="@Stylesheet.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ca1.AlternateViewsController">
   <children>
      <Label layoutX="225.0" layoutY="-5.0" text="Alternative Views">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="back" mnemonicParsing="false" onAction="#back" text="Back" />
      <TabPane layoutX="15.0" layoutY="39.0" prefHeight="343.0" prefWidth="570.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.topAnchor="39.0">
        <tabs>
          <Tab text="Component Color">
            <content>
              <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefHeight="332.0" prefWidth="570.0">
                     <children>
                        <ImageView fx:id="componentColor" fitHeight="468.0" fitWidth="527.0" layoutX="22.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Random Color">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fx:id="randomColor" fitHeight="468.0" fitWidth="527.0" layoutX="22.0" pickOnBounds="true" preserveRatio="true" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
